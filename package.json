{
  "name": "feathers-opcua-server",
  "description": "A FeathersJS OpcUA application",
  "version": "4.0.0",
  "homepage": "https://feathersjs.com",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "BSA Lab.",
    "email": "bs261257@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 11.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "test:all": "npm run eslint && npm run mocha ",
    "test:debug": "npm run eslint:debug && npm run mocha:debug",
    "coverage": "nyc npm run mocha",
    "mocha": "npm run clean && cross-env NODE_ENV=test DEBUG=app:* mocha test/ --recursive --exit",
    "mocha:debug": "npm run clean && cross-env NODE_ENV=test DEBUG=app:* mocha test-debug/ --recursive --exit",
    "eslint": "eslint src/. test/.  --config .eslintrc.json",
    "eslint:debug": "eslint src/. test-debug/.  --config .eslintrc.json",
    "eslint-fix": "eslint --fix src/. test/. --config .eslintrc.json",
    "eslint-fix:debug": "eslint --fix src/. test-debug/. --config .eslintrc.json",
    "clean": "shx rm -rf test/data/*.db",
    "shx": "shx rm test/data/*.db",
    "dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon src/",
    "start": "node src/",
    "update-dependencies": "ncu -u"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.3",
    "@feathersjs/authentication-local": "^4.5.4",
    "@feathersjs/authentication-oauth": "^4.5.4",
    "@feathersjs/configuration": "^4.5.3",
    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio": "^4.5.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "feathers-nedb": "^5.1.0",
    "helmet": "^3.22.0",
    "moment": "^2.27.0",
    "nedb": "^1.8.0",
    "node-opcua": "^2.14.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "eslint": "^7.1.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^6.0.1",
    "nyc": "^15.0.1",
    "shx": "^0.3.2"
  }
}
