{
  "app": {
    "envTestModeName": "test",
    "envAllowingSeedData": ["development", "test"],
    "envAllowingOpcuaBootstrap": ["development", "production"],
    "seedData": true,
    "name": "feathers-opcua-server",
    "description": "Project feathers-opcua-server",
    "src": "src",
    "providers": [
      "rest",
      "socketio"
    ]
  },
  "services": {
    "users": {
      "name": "users",
      "nameSingular": "user",
      "subFolder": "",
      "fileName": "users",
      "adapter": "nedb",
      "path": "/users",
      "isAuthEntity": true,
      "requiresAuth": true
    },
    "roles": {
      "name": "roles",
      "nameSingular": "role",
      "subFolder": "",
      "fileName": "roles",
      "adapter": "nedb",
      "path": "/roles",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    },
    "userProfiles": {
      "name": "userProfiles",
      "nameSingular": "userProfile",
      "subFolder": "",
      "fileName": "user-profiles",
      "adapter": "nedb",
      "path": "/user-profiles",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    },
    "teams": {
      "name": "teams",
      "nameSingular": "team",
      "subFolder": "",
      "fileName": "teams",
      "adapter": "nedb",
      "path": "/teams",
      "isAuthEntity": false,
      "requiresAuth": true
    },
    "userTeams": {
      "name": "userTeams",
      "nameSingular": "userTeam",
      "subFolder": "",
      "fileName": "user-teams",
      "adapter": "nedb",
      "path": "/user-teams",
      "isAuthEntity": false,
      "requiresAuth": true
    },
    "logMessages": {
      "name": "logMessages",
      "nameSingular": "logMessage",
      "subFolder": "",
      "fileName": "log-messages",
      "adapter": "nedb",
      "path": "/log-messages",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    }
  },
  "hooks": {
    "normalize": {
      "fileName": "normalize",
      "camelName": "normalize",
      "ifMulti": "y",
      "multiServices": [
        "*app"
      ],
      "singleService": ""
    },
    "log": {
      "fileName": "log",
      "camelName": "log",
      "ifMulti": "y",
      "multiServices": [
        "*app"
      ],
      "singleService": ""
    }
  }
}
